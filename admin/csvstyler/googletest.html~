<html xmlns="http://www.w3.org/1999/xhtml">
    <head>
        <meta http-equiv="content-type" content="text/html; charset=utf-8"/>
        <title>Google Maps with GeoWebCache</title>
        
        <style>
          body {
              font-family: verdana, arial, helvetica, sans-serif;
              font-size: 10pt
          }
        </style>
        
        <script src='http://maps.google.com/maps?file=api&amp;v=2&amp;key=ABQIAAAA0CG8rV5Lkr_jNKdhuF2gLRSRDD4uGs4FcOGl1U5QSBKeQAPKXBQIVEr-Kx3sfnr5mQXl0Ek7D3Y5Gg'></script>        <script type="text/javascript">

        function initialize() {  
            if (GBrowserIsCompatible()) {
                var map = new GMap2(document.getElementById("map_canvas"));
                map.setCenter(new GLatLng(37.27183723449705, -119.2720069885255), 5);

                var tilelayer =  new GTileLayer(null, null, null, {
                    tileUrlTemplate: location.search.substr(1), 
                            isPng:true,
                            opacity:0.7 }
                );
                       
                var myTileLayer = new GTileLayerOverlay(tilelayer);
                map.addOverlay(myTileLayer);
                
                document.getElementById("url").innerHTML = location.search.substr(1);
            }
        }
    
        </script>
    </head>
    <body onload="initialize()" onunload="GUnload()">
        <div id="map_canvas" style="width: 500px; height: 300px"></div>
        <p><b>Paste the url below into your application to use this overlay:</b><br/>
        <span id="url"/></span>
    </body>
